Compression yes

# Refresh connection every 60 seconds for a maximum of one hour
TCPKeepAlive yes
ServerAliveInterval 120

# disconnect if server doesn't respond after 4 minutes (120*2) 
ServerAliveCountMax 2

# use ssh-agent
AddKeysToAgent yes

Host keithieopia
    HostName 45.33.77.38
    User timothy
    IdentityFile ~/.ssh/id_home

